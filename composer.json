{
  "description": "Laravel blog content with vuejs support.",
  "name": "bantenprov/vue-blog",
  "type": "library",
  "homepage": "https://github.com/bantenprov/vue-blog",
  "authors": [
    {
      "name": "bantenprov",
      "email": "developer.bantenprov@gmail.com",
      "role": "developer"
    }
  ],
  "license": "MIT",
  "support": {
    "source": "https://github.com/bantenprov/vue-blog",
    "issues": "https://github.com/bantenprov/vue-blog/issues",
    "wiki": "https://github.com/bantenprov/vue-blog/wiki"
  },
  "require": {
    "php": ">=5.6.4",
    "illuminate/support": "5.4.*",
    "laravelcollective/html":"^5.4.0"
  },
  "autoload": {
        "psr-4": {
            "Bantenprov\\VueBlog\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/phpspec": "~2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Bantenprov\\VueBlog\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
